        <attr name="slider_background" format="color" />
        <attr name="slider_active_color" format="color" />
        <attr name="corner_radius" format="dimension" />
        <attr name="rectangle_height" format="dimension" />

        <attr name="slider_step_size" format="float" />
        <attr name="slider_from_progress" format="float" />
        <attr name="slider_to_progress" format="float" />
        <attr name="slider_min_value" format="float" />
        <attr name="slider_max_value" format="float" />

        <attr name="thumb_size" format="dimension" />
        <attr name="thumb_color" format="color" />
        <attr name="thumb_second_color" format="color" />
        <attr name="thumb_single_color" format="boolean" />
